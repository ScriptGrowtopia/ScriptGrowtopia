end

    if packet:find("action|dialog_return\ndialog_name|wh\nbuttonClicked|wpull") or packet:find("/wr1") then
        if pull == false then
            pull = true
            kick = false
            ban = false
            ovlay("Pull Mode Enable")
            else
                pull = false
                kick = false
                ban = false
                ovlay("Pull Mode Disable")
                return true
        end
    end
    if packet:find("action|dialog_return\ndialog_name|wh\nbuttonClicked|wban") or packet:find("/wr2") then
